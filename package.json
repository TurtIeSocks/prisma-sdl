  },
  "keywords": [
    "prisma",
    "graphql",
    "gql",
    "sdl",
    "generator",
    "react",
    "hooks",
    "apollo"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/TurtIeSocks"
  },
  "scripts": {
    "start": "node .",
